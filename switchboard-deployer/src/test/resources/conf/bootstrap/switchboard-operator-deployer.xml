<?xml version="1.0" encoding="UTF-8"?>
<deployment xmlns="urn:jboss:bean-deployer:2.0">
    <bean name="EJBSwitchBoardOperatorDeployer" class="org.jboss.injection.naming.deployer.EJBSwitchBoardOperatorDeployer"/>
    <bean name="WebSwitchBoardOperatorDeployer" class="org.jboss.injection.naming.deployer.WebSwitchBoardOperatorDeployer"/>    

    <bean name="EnvironmentProcessor" class="org.jboss.injection.resolve.naming.EnvironmentProcessor">
        <incallback method="addResolver"/>
        <incallback method="addMetaDataVisitor"/>
    </bean>


    <!-- Mocks -->
    <bean name="JavaEEComponentInformer" class="org.jboss.injection.naming.test.deployer.support.MockJavaEEComponentInformer"/>
    <bean name="MockEjbComponentDeployer" class="org.jboss.injection.naming.test.deployer.support.MockEjbComponentDeployer"/>
    <bean name="MockEjbReferenceResolver" class="org.jboss.injection.naming.test.deployer.support.MockAnnotatedEjbReferenceResolver"/>
    <bean name="MockEnvironmentEntryResolver" class="org.jboss.injection.naming.test.deployer.support.MockEnvironmentEntryResolver"/>
</deployment>